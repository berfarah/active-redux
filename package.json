    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-jest": "^19.0.1",
    "jest": "^20.0.3",
    "jest-cli": "^20.0.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "react-redux": "^5.0.0",
